<frost-login-form>
	<h3>Login</h3>
	<form>
		<label for="screen_name">Username</label>
		<input class="screen-name-box" type='text' name='screen_name' placeholder='example: frost_abc' style="width: 100%" required />
		<label for="password">Password</label>
		<input class="password-box" type='password' name='password' style="width: 100%" required />
		<input class="button-primary" type="submit", value="Login" />
	</form>

	<style />
</frost-login-form>
